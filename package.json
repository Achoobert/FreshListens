{
  "name": "freshlistens",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron ."
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "electron-rebuild": "^1.11.0",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-dist": "^1.12.1",
    "jstree": "^3.3.9",
    "popper.js": "^1.14.4",
    "zerorpc": "git+https://github.com/0rpc/zerorpc-node.git"
  },
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-packager": "^12.2.0"
  },
  "description": "Allow a single PC to admin files for different listeners",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "freshlistens"
  },
  "keywords": [
    "DigiServe",
    "Audio",
    "Library"
  ],
  "author": "Isaac Schubert",
  "license": "OSL",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "prune": true,
        "icon": "src/icons/icon",
        "ignore": [
          "/src/pycalc$",
          "\\.git(ignore|config)",
          "\\.vscode",
          "\\.idea",
          "/package-lock\\.json",
          "Pipfile($|\\.lock)",
          "/(api_pythermal|api|prtail)\\.spec",
          "/bin$",
          "/build$",
          "/out2$",
          "/Output$",
          "/README\\.md"
        ]
      }
    }
  }
}
